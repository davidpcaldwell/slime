{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "eslint",
			"type": "shell",
			"command": "./jsh.bash contributor/eslint.jsh.js",
			"problemMatcher": [],
			"presentation": {
				"echo": false,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": true
			}
		},
		{
			"label": "documentation",
			"type": "shell",
			"command": "./jsh.bash tools/documentation.jsh.js -base . -host slime",
			"problemMatcher": []
		},
		{
			"label": "hosting",
			"type": "shell",
			"command": "./jsh.bash contributor/hosting.jsh.js",
			"problemMatcher": []
		},
		{
			"label": "Fifty UI: SLIME",
			"type": "shell",
			"command": "loader/api/test/fifty/ui.bash",
			"problemMatcher": []
		},
		{
			"label": "Fifty UI harness",
			"type": "shell",
			"command": "loader/api/test/fifty/ui.bash --harness",
			"problemMatcher": []
		},
		{
			"label": "Fifty tsc harness",
			"detail": "Runs the UI, opening the tsc JSON page",
			"type": "shell",
			"command": "loader/api/test/fifty/ui.bash --tsc-harness",
			"problemMatcher": []
		},
		{
			"label": "Fifty tsc harness: debug",
			"detail": "Runs the tsc node.js program on test data using a debugger",
			"type": "shell",
			"command": "node --inspect-brk loader/api/test/fifty/tsc.node.js loader/api/test/fifty/test/data/module.d.ts; echo \"Terminated with exit status $?.\"",
			"options": {
				"env": {
					"PROJECT": ".",
					"PATH": "${PATH}:local/jsh/lib/node/bin",
					"NODE_PATH": "local/jsh/lib/node/lib/node_modules"
				}
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "dedicated",
				"showReuseMessage": false,
				"clear": true
			},
			"problemMatcher": []
		}
	]
}