{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Node.js Debug (attach 9229)",
			"type": "node",
			"request": "attach",
			"port": 9229
		},
		{
			"type": "java",
			"name": "Java JDWP Debug (attach 8000)",
			"request": "attach",
			"hostName": "localhost",
			"port": 8000
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "ncdbg",
			"address": "localhost",
			"port": 7778,
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Chrome debug (attach 9222)",
			"type": "pwa-chrome",
			"request": "attach",
			"port": 9222,
			"webRoot": "${workspaceFolder}"
		}
	]
}